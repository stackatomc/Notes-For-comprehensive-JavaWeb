<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>JspAndServlet</servlet-name>
    <servlet-class>servlet.JspAndServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JspAndServlet</servlet-name>
    <url-pattern>/servlet/jspAndservlet</url-pattern>
  </servlet-mapping>
    
    <servlet>
        <servlet-name>EnterServlet</servlet-name>
        <servlet-class>servlet.EnterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EnterServlet</servlet-name>
        <url-pattern>/servlet/enterservlet</url-pattern>
    </servlet-mapping>
</web-app>
